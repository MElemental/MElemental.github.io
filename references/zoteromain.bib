  author = {Monzani, Paulo S. and Sangalli, Juliano R. and Sampaio, Rafael V. and Guemra, Samuel and Zanin, Renato and Adona, Paulo R. and Berlingieri, Maria A. and Cunha-Filho, Luiz F. C. and Mora- Ocampo, Irma Y. and Pirovani, Carlos P. and Meirelles, Fl{\'a}vio V. and Wheeler, Matthew B. and Ohashi, Otavio M.},
