@article{qinAmmoniaoxidizingBacteriaArchaea2024,
  title = {Ammonia-Oxidizing Bacteria and Archaea Exhibit Differential Nitrogen Source Preferences},
  author = {Qin, Wei and Wei, Stephany P. and Zheng, Yue and Choi, Eunkyung and Li, Xiangpeng and Johnston, Juliet and Wan, Xianhui and Abrahamson, Britt and Flinkstrom, Zachary and Wang, Baozhan and Li, Hanyan and Hou, Lei and Tao, Qing and Chlouber, Wyatt W. and Sun, Xin and Wells, Michael and Ngo, Long and Hunt, Kristopher A. and Urakawa, Hidetoshi and Tao, Xuanyu and Wang, Dongyu and Yan, Xiaoyuan and Wang, Dazhi and Pan, Chongle and Weber, Peter K. and Jiang, Jiandong and Zhou, Jizhong and Zhang, Yao and Stahl, David A. and Ward, Bess B. and Mayali, Xavier and {Martens-Habbena}, Willm and Winkler, Mari-Karoliina H.},
  year = {2024},
  month = jan,
  journal = {Nature Microbiology},
  volume = {9},
  number = {2},
  pages = {524--536},
  issn = {2058-5276},
  doi = {10.1038/s41564-023-01593-7},
  url = {https://www.nature.com/articles/s41564-023-01593-7},
  urldate = {2024-03-19},
  abstract = {Ammonia-oxidizing microorganisms (AOM) contribute to one of the largest nitrogen fluxes in the global nitrogen budget. Four distinct lineages of AOM: ammonia-oxidizing archaea (AOA), beta- and gamma-proteobacterial ammonia-oxidizing bacteria ({$\beta$}-AOB and {$\gamma$}-AOB) and complete ammonia oxidizers (comammox), are thought to compete for ammonia as their primary nitrogen substrate. In addition, many AOM species can utilize urea as an alternative energy and nitrogen source through hydrolysis to ammonia. How the coordination of ammonia and urea metabolism in AOM influences their ecology remains poorly understood. Here we use stable isotope tracing, kinetics and transcriptomics experiments to show that representatives of the AOM lineages employ distinct regulatory strategies for ammonia or urea utilization, thereby minimizing direct substrate competition. The tested AOA and comammox species preferentially used ammonia over urea, while {$\beta$}-AOB favoured urea utilization, repressed ammonia transport in the presence of urea and showed higher affinity for urea than for ammonia. Characterized {$\gamma$}-AOB co-utilized both substrates. These results reveal contrasting niche adaptation and coexistence patterns among the major AOM lineages.},
  copyright = {2024 The Author(s), under exclusive licence to Springer Nature Limited},
  langid = {american},
  language = {en-US},
  keywords = {Biogeochemistry,Microbial ecology},
  annotation = {3 citations (Crossref) [2024-03-24]\\
Read\_Status: New\\
Read\_Status\_Date: 2024-03-19T13:58:22.373Z},
  file = {zotero://open-pdf/library/items/RHQQEGVZ?Qin et al 2023.pdf}
}

